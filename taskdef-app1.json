{
  "family": "app1-task",
  "networkMode": "awsvpc",
  "requiresCompatibilities": ["FARGATE"],
  "cpu": "256",
  "memory": "512",
  "executionRoleArn": "arn:aws:iam::132398229882:role/posistrength-dev-ecs-task-execution-role",
  "containerDefinitions": [
    {
      "name": "app1",
      "image": "IMAGE1_NAME",
      "essential": true,
      "portMappings": [
        {
          "containerPort": 80,
          "protocol": "tcp"
        }
      ]
    }
  ]
}
