{
  "family": "app1-task",
  "networkMode": "awsvpc",
  "requiresCompatibilities": ["FARGATE"],
  "cpu": "256",
  "memory": "512",
  "executionRoleArn": "arn:aws:iam::132398229882:role/posistrength-dev-ecs-task-execution-role",
  "taskRoleArn": "arn:aws:ecs:ap-southeast-1:132398229882:task-definition",
  "containerDefinitions": [
    {
      "name": "app1",
      "image": "132398229882.dkr.ecr.ap-southeast-1.amazonaws.com/posistrength-dev-webposistrength-ecr",
      "essential": true,
      "portMappings": [
        {
          "containerPort": 80,
          "hostPort": 80,
          "protocol": "tcp"
        }
      ]
    }
  ]
}
