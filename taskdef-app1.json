{
  "family": "app1-task",
  "networkMode": "awsvpc",
  "requiresCompatibilities": ["FARGATE"],
  "cpu": "256",
  "memory": "512",
  "executionRoleArn": "arn:aws:iam::132398229882:role/ecsTaskExecutionRole",
  "taskRoleArn": "arn:aws:iam::132398229882:role/ecsTaskRole",
  "containerDefinitions": [
    {
      "name": "app1",
      "image": "nginx:latest"
      "portMappings": [
        {
          "containerPort": 80,
          "protocol": "tcp"
        }
      ],
      "essential": true
    }
  ]
}
